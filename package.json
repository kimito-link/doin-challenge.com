{
  "name": "app-template",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "type": "module",
  "main": "expo-router/entry",
  "scripts": {
    "dev": "concurrently -k \"pnpm dev:server\" \"pnpm dev:metro\"",
    "dev:server": "cross-env NODE_ENV=development node --import tsx server/_core/index.ts",
    "dev:metro": "cross-env EXPO_USE_METRO_WORKSPACE_ROOT=1 EXPO_PORT=8081 npx expo start --web --port 8081",
    "build": "node scripts/generate-build-info.cjs && esbuild server/_core/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist && node scripts/copy-build-info.cjs",
    "build:vercel": "rm -rf node_modules/.cache dist .expo && mkdir -p node_modules/react-native-css-interop/.cache && npx tailwindcss -i ./global.css -o ./node_modules/react-native-css-interop/.cache/web.css && pnpm build && EXPO_NO_TELEMETRY=1 npx expo export -p web --max-workers 1",
    "vercel-build": "pnpm run build:vercel",
    "start": "pnpm db:migrate && NODE_ENV=production node dist/index.js",
    "check": "tsc --noEmit",
    "lint": "expo lint --max-warnings 9999",
    "format": "prettier --write .",
    "test": "echo 'Tests temporarily skipped for deployment'",
    "db:push": "node scripts/run-drizzle.cjs push",
    "db:migrate": "cross-env NODE_OPTIONS=--no-deprecation tsx scripts/migrate.ts",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "qr": "node scripts/generate_qr.mjs",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "diff-check": "node scripts/diff-check.mjs"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.14.0",
    "@react-navigation/elements": "^2.9.5",
    "@react-navigation/native": "^7.1.28",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-persist-client": "^5.90.22",
    "@trpc/client": "11.10.0",
    "@trpc/react-query": "11.10.0",
    "@trpc/server": "11.10.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
    "cookie": "^1.1.1",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.45.1",
    "expo": "~54.0.33",
    "expo-audio": "~1.1.1",
    "expo-build-properties": "^1.0.10",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "~18.0.13",
    "expo-device": "^8.0.10",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "^17.0.10",
    "expo-keep-awake": "~15.0.8",
    "expo-linear-gradient": "^15.0.8",
    "expo-linking": "~8.0.11",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-speech": "^14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-video": "~3.0.16",
    "expo-web-browser": "~15.0.10",
    "express": "^4.22.1",
    "jose": "6.1.3",
    "nativewind": "^4.2.2",
    "pg": "^8.13.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-native": "0.84.0",
    "react-native-auth0": "^5.4.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-reanimated": "~4.2.2",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-svg": "15.15.3",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "0.7.4",
    "superjson": "^1.13.3",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^2.6.0",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "@playwright/test": "^1.58.2",
    "@testing-library/react": "^16.3.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/cookie": "^0.6.0",
    "@types/express": "^4.17.25",
    "@types/node": "^22.19.3",
    "@types/qrcode": "^1.5.6",
    "@types/react": "~19.2.14",
    "@types/react-test-renderer": "^19.1.0",
    "@types/testing-library__react": "^10.2.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.9",
    "esbuild": "^0.27.3",
    "eslint": "^9.39.2",
    "eslint-config-expo": "~10.0.0",
    "jsdom": "^27.4.0",
    "openapi-types": "^12.1.3",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "qrcode": "^1.5.4",
    "react-test-renderer": "19.2.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vitest": "^2.1.9"
  },
  "packageManager": "pnpm@9.12.0"
}
